<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.kr.lotteon.mapper.cs.LtCsCateMapper">

	<select id="selectCsCate1" resultType="co.kr.lotteon.dto.LtCsCate1DTO">
		SELECT * FROM `lt_cs_cate1` WHERE `cate1` >=20
	</select>

	<select id="selectCsCate2" resultType="co.kr.lotteon.dto.LtCsCate2DTO">
		SELECT * FROM `lt_cs_cate2` WHERE `cate1` =#{cate1}
	</select>

</mapper>
